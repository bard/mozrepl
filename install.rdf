    <!-- toolkit -->
    <em:targetApplication>
      <Description>
        <em:id>toolkit@mozilla.org</em:id>
        <em:minVersion>1.9</em:minVersion>
        <em:maxVersion>20.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:name>MozRepl</em:name>
    <em:description>Interactive console for Mozilla applications</em:description>
    <em:creator>Massimiliano Mirra</em:creator>
    <em:contributor>Luca Greco</em:contributor>
    <em:homepageURL>https://github.com/bard/mozrepl/wiki/</em:homepageURL>
  </Description>
</RDF>
